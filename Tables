create table RETURN
(
ReturnID CHAR(25) NOT NULL,
ProductID CHAR(25) NOT NULL,
ReturnReason VARCHAR2(25),
ReturnDate Date not null,
constraint RETURN_PK PRIMARY KEY (ReturnID),
constraint RETURN_FK FOREIGN KEY (ProductID) REFERENCES Product (ProductID)
);

//* STORE TABLE
Create TABLE STORE (
StoreID char (25) not null,
StoreName varchar (255),
StoreAddress varchar2 (100),
primary key (StoreID)
);

//* EMPLOYEE TABLE
Create TABLE Employee (
EmpID int not null,
employeeFirstName varchar2 (25),
employeeLastName varchar2 (25),
primary key (EmpID)
);

//* SUPPLIER TABLE
Create TABLE Supplier (
SupplierID int not null,
SupplierName varchar2 (25),
SupplierEmail varchar2 (255),
primary key (SupplierID)
);

//* WAREHOUSE
create table WAREHOUSE(
WareHouseID CHAR(25) NOT NULL,
ProductID CHAR(25) NOT NULL,
capacity VARCHAR2(25),
constraint WAREHOUSE_PK PRIMARY KEY (WareHouseID),
constraint WAREHOUSE_FK FOREIGN KEY (ProductID) REFERENCES Product (ProductID)
);

//* ORDER
create table ORDERT(
OrderID CHAR(25) NOT NULL,
WarehouseID CHAR(25) NOT NULL,
ProductID CHAR(25) NOT NULL,
SupplierID CHAR(25) NOT NULL,
constraint ORDERT_PK PRIMARY KEY (WareHouseID),
constraint ORDERT_FK FOREIGN KEY (WarehouseID) REFERENCES Product (WarehouseID)
constraint ORDERT_FK FOREIGN KEY (ProductID) REFERENCES Product (ProductID)
constraint ORDERT_FK FOREIGN KEY (SupplierID) REFERENCES Product (SupplierID)
);

//* TRANSPORTATION
create table TRANSPORTATION(
TransportID CHAR(25) NOT NULL,
transportType CHAR(25) NOT NULL,
constraint TRANSPORTATION_PK PRIMARY KEY (TransportID)
);

//* PRODUCT
create table PRODUCT(
ProductID CHAR(25) NOT NULL,
ProductType VARCHAR2(25),
ProductName VARCHAR2(25),
constraint PRODUCT_PK PRIMARY KEY (ProductID)
);


//* PURCHASE
create table PURCHASE(
PurchID CHAR(25) NOT NULL,
ProductID VARCHAR2(25) NOT NULL,
PurchNumber VARCHAR2(25),
PurchaseDate VARCHAR2(25),
associate_ID VARCHAR2(25),
constraint PURCHASE_PK PRIMARY KEY (PurchID)
constraint PURCHASE_FK FOREIGN KEY (ProductID) REFERENCES Product (ProductID)
);


//* CUSTOMER
create table CUSTOMER(
CustID CHAR(25) NOT NULL,
FirstName VARCHAR2(25),
LastName VARCHAR2(25),
Address VARCHAR2(255),
Phone VARCHAR2(255),
Email VARCHAR2(255),
CreditCardTypeID VARCHAR2(25),
constraint CUSTOMER_PK PRIMARY KEY (ProductID)
);


//* PAYMENT
create table PAYMENT(
pmt_ID CHAR(25) NOT NULL,
paymentAmount Char(25),
pmtType CHAR(25),
ChargedDate DATE,
);

//* WOMEN
create table WOMEN(
ProductID CHAR(25) NOT NULL,
WomenClothingType VARCHAR2(255),
PurchasID CHAR(25) NOT NULL,
CustID CHAR(25) NOT NULL,
EmpID NUMBER NOT NULL,
CONSTRAINT Women_PK PRIMARY KEY (ProductID),
CONSTRAINT Women_FK1 FOREIGN KEY (PurchasID) REFERENCES PURCHASE (PurchasID),
CONSTRAINT Women_FK2 FOREIGN KEY (CustID) REFERENCES CUSTOMER (CustID),
CONSTRAINT Women_FK3 FOREIGN KEY (EmpID) REFERENCES Employee (EmpID)
);


//* MEN
create table MEN(
ProductID CHAR(25) NOT NULL,
MenClothingType VARCHAR2(255),
PurchasID CHAR(25) NOT NULL,
CustID CHAR(25) NOT NULL,
EmpID NUMBER NOT NULL,
CONSTRAINT Men_PK PRIMARY KEY (ProductID),
CONSTRAINT Men_FK1 FOREIGN KEY (PurchasID) REFERENCES PURCHASE (PurchasID),
CONSTRAINT Men_FK2 FOREIGN KEY (CustID) REFERENCES CUSTOMER (CustID),
CONSTRAINT Men_FK3 FOREIGN KEY (EmpID) REFERENCES Employee (EmpID)
);

//* KIDS
create table KIDS(
ProductID CHAR(25) NOT NULL,
KidsClothingType VARCHAR2(255),
PurchID CHAR(25) NOT NULL,
CustID CHAR(25) NOT NULL,
EmpID NUMBER NOT NULL,
CONSTRAINT Kids_PK PRIMARY KEY (ProductID),
CONSTRAINT Kids_FK1 FOREIGN KEY (PurchID) REFERENCES PURCHASE (PurchID),
CONSTRAINT Kids_FK2 FOREIGN KEY (CustID) REFERENCES CUSTOMER (CustID),
CONSTRAINT Kids_FK3 FOREIGN KEY (EmpID) REFERENCES Employee (EmpID)
);

